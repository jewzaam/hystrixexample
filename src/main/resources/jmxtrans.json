{
    "queries": [
    {
        "objectName": "com.netflix.servo:name=*,type=COUNTER",
        "resultAlias": "servo.%name%",
        "attributes": [
            {
                "name": "value",
                "type": "counter",
                "resultAlias": "counter"
            }
        ]
    },
    {
        "objectName": "com.netflix.servo:name=*,type=INFO",
        "resultAlias": "servo.%name%",
        "attributes": [
            {
                "name": "value",
                "type": "gauge",
                "resultAlias": "gauge"
            }
        ]
    },
    {
        "objectName": "com.netflix.servo:name=*,type=GAUGE",
        "resultAlias": "servo.%name%",
        "attributes": [
            {
                "name": "value",
                "type": "gauge",
                "resultAlias": "gauge"
            }
        ]
    }
],
    "outputWriters": [
    {
        "@class": "org.jmxtrans.embedded.output.GraphiteWriter",
        "settings": {
            "port": 8080,
            "host": "metrics11.devlab.redhat.com"
        }
    }
],
    "queryIntervalInSeconds": "${jmxtrans.queryIntervalInSeconds:30}",
    "numQueryThreads": "${jmxtrans.numQueryThreads:1}",
    "numExportThreads": "${jmxtrans.numExportThreads:2}",
    "exportIntervalInSeconds": "${jmxtrans.exportIntervalInSeconds:5}",
    "exportBatchSize": "${jmxtrans.exportBatchSize:50}"
}
